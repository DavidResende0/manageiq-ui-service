{
  "name": "manageiq-ui-service",
  "description": "ManageIQ Service dependencies",
  "homepage": "https://manageiq.org/",
  "author": "Red Hat",
  "license": "Apache-2.0",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/ManageIQ/manageiq-ui-service.git"
  },
  "version": "1.0.1",
  "scripts": {
    "build": "yarn build:git-hash && NODE_ENV=\"production\" webpack --bail --progress --profile --config config/webpack.prod.js",
    "build:git-hash": "node config/githash.js",
    "build:static": "webpack --bail --progress --profile --config config/webpack.static.js",
    "available-languages": "cd ./config/ && node available-languages.js",
    "heroku-postbuild": "yarn build:static",
    "gettext:compile": "angular-gettext-cli --compile --files './client/gettext/po/**/*.po' --dest 'client/gettext/json/manageiq-ui-service.json' --format json && yarn available-languages",
    "gettext:extract": "angular-gettext-cli --files './client/**/*.+(js|html)' --exclude '**/*.+(spec.js|e2e.js)' --dest './client/gettext/po/manageiq-ui-service.pot' --marker-names '__,N_'",
    "gettext:validate": "node language/validate.js",
    "start": "webpack-dev-server --open --progress --config config/webpack.dev.js",
    "start:mock-api": "cd node_modules/@manageiq/manageiq-api-mock/ && node index.js",
    "start:mock-webserver": "cd server && node app.js",
    "start:all": "yarn start:mock-api & yarn start:mock-webserver",
    "test": "yarn vet && yarn gettext:extract && yarn gettext:validate && karma start karma.conf.js --single-run && yarn test:jest",
    "test:jest": "jest /client/.*test.js",
    "test:e2e": "protractor protractor.conf.js",
    "test:watch": "karma start --auto-watch --no-single-run",
    "vet": "standard && sass-lint --verbose --no-exit",
    "zanata:upload": "yarn gettext:extract && yarn gettext:compile && node language/index.js upload",
    "zanata:download": "node language/index.js download"
  },
